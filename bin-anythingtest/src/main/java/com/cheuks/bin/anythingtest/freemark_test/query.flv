<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE QueryList SYSTEM "Query.dtd">
<QueryList package="${entityFullName}">
	<Alias name="${entityFullName}" Alias="${entitySimpleName}" />
	<Query name="list" type="HQL" freemarkFormat="true" Alias="true">
		<![CDATA[ 
			FROM ${entitySimpleName} A 
			WHERE 
			1=1
		<#list params as item> 
			<${tag}if ${item}??>
				<${tag}if like??>
					and A.${item} like '%'||:${item}||'%' 
				<${tag}else>
					and A.${item}=:${item} 
				</${tag}if>
			</${tag}if>
		</#list>
		]]>
	</Query>
</QueryList>