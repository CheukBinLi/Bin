import project.master.dbmaamger.DBAdapter;
import project.master.dbmaamger.dao.AbstractDao;

@Component
public class ${entityName}DaoImpl extends AbstractDao<${entityName}, ${idType}> implements ${entityName}Dao {

	@Autowired
	private DBAdapter dBAdapter;

	@Override
	public Class<${entityName}> getEntityClass() {
